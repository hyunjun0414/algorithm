#include <stdio.h>
void Swap(int& a, int& b)
{
	int temp = a;
	a = b;
	b = temp;
}
void PrintList(const int list[], int size)
{
	for (int i = 0; i < size; i++)
		printf("%5d", list[i]);
	printf("\n\n");
}
void QuickSort(int list[], int size)
{

}
int main() {
	int list[100] = { 0 };
	int size = 100;

	
	PrintList(list, size);
	QuickSort(list, size);
	PrintList(list, size);
}
